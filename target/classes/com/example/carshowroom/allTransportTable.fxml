<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="594.0" prefWidth="1224.0" style="-fx-background-color: #212f3d;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.carshowroom.allTransportTablle">
    <children>
        <TableView fx:id="Transport" layoutY="45.0" prefHeight="550.0" prefWidth="1224.0">
            <columns>
                <TableColumn fx:id="idTC" prefWidth="125.0" text="id T/С" />
                <TableColumn fx:id="type_transport" prefWidth="160.0" text="Тип Т/С" />
                <TableColumn fx:id="brand_transport" prefWidth="183.0" text="Бренд Т/С" />
                <TableColumn fx:id="model_transport" minWidth="0.0" prefWidth="147.0" text="Модель Т/С" />
                <TableColumn fx:id="vin_transport" minWidth="0.0" prefWidth="185.0" text="VIN" />
                <TableColumn fx:id="dateOfManufactureTransport" minWidth="0.0" prefWidth="234.0" text="Дата Изготовления Т/С" />
            <TableColumn fx:id="priceTransport" prefWidth="188.0" text="Цена" />
            </columns>
        </TableView>
        <Button fx:id="buttonBack" layoutX="14.0" layoutY="10.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="109.0" text="назад" />
        <Button fx:id="dellTransport" layoutX="146.0" layoutY="10.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="109.0" text="Удалить" />
    </children>
</AnchorPane>
